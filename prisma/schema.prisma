generator client {
    provider = "prisma-client"
    output   = "../generated/prisma"
	engineType = "client"
}

datasource db {
    provider = "mysql"
}

model Notafiscal {
    id          String  @id @unique
    description String  @default("")
    link        String  @db.VarChar(750)
    data        String
    check       Boolean @default(false)
}
